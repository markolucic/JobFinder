<h1><%= @job.title %></h1>
<p>
	<strong>Description:</strong>
	<%= @job.description %>
</p>
<p>
	<strong>Email:</strong>
	<%= @job.email %>
</p>
<p>
	<strong>City:</strong>
	<%= @job.city %>
</p>
<p>
	<strong>Salary:</strong>
	<%= @job.salary %> KM
</p>
<p>
	<strong>Experience:</strong>
	<%= @job.experience ? "Yes" : "No" %>
</p>
<%= link_to 'Delete Job', job_path(@job), method: :delete, data: {confirm: 'Are you sure?' }, class:"btn btn-danger"%>
</br></br>
<%= link_to 'Back to Jobs', jobs_path %>